{
  "name": "@emonkak/enumerable",
  "version": "4.0.0",
  "description": "LINQ to Object for JavaScript that provides the individual methods as a module.",
  "main": "index.js",
  "scripts": {
    "build": "tsc --declaration && cp LICENSE README.md package.json dist/",
    "cover": "nyc npm test",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rm -f -r dist",
    "precover": "rm -f -r .nyc_output coverage",
    "test": "mocha --recursive --compilers ts:ts-node/register",
    "watch": "tsc --watch"
  },
  "author": "Shota Nozaki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:emonkak/js-enumerable.git"
  },
  "homepage": "https://github.com/emonkak/js-enumerable",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.14",
    "@types/sinon": "^7.5.1",
    "cash-cp": "^0.2.0",
    "cash-rm": "^0.2.0",
    "coveralls": "^3.0.8",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "dist",
      "node_modules",
      "test"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "all": true
  }
}
