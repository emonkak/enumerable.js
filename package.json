{
  "name": "@emonkak/enumerable",
  "version": "2.2.0",
  "description": "LINQ to Object for JavaScript that provides the individual methods as a module.",
  "main": "index.js",
  "scripts": {
    "build": "tsc --declaration && cp LICENSE README.md package.json dist/",
    "cover": "nyc npm test",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rm -f -r dist",
    "precover": "rm -f -r .nyc_output coverage",
    "test": "mocha --recursive --compilers ts:ts-node/register",
    "watch": "tsc --watch"
  },
  "author": "Shota Nozaki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:emonkak/js-enumerable.git"
  },
  "homepage": "https://github.com/emonkak/js-enumerable",
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.31",
    "@types/sinon": "^2.3.5",
    "cash-cp": "^0.2.0",
    "cash-rm": "^0.2.0",
    "coveralls": "^3.0.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "sinon": "^4.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "dist",
      "node_modules",
      "test"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "all": true
  }
}
